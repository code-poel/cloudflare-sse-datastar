{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cloudflare-sse-datastar",
	"compatibility_date": "2025-05-09",
	"main": "src/index.ts",
	"assets": {
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"rules": [
		{
      "type": "Text",
      "globs": [
        "**/*.mustache"
      ],
      "fallthrough": true
    }
  ]
}